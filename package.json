{
  "name": "pagination-engine",
  "version": "1.0.0",
  "description": "A comprehensive and robust pagination engine for React applications, similar to Google Docs/Apple Pages",
  "type": "module",
  "main": "./dist/pagination-engine.umd.js",
  "module": "./dist/pagination-engine.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/pagination-engine.es.js",
      "require": "./dist/pagination-engine.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "pagination",
    "react",
    "document",
    "layout",
    "page-break",
    "pdf",
    "printing",
    "typescript"
  ],
  "author": "Pagination Engine Contributors",
  "license": "MIT",
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.0",
    "vite-plugin-dts": "^4.5.4"
  }
}
